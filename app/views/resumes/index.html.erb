<p id="notice"><%= notice %></p>

<h1>Resumes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Dob</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Gender</th>
      <th>Qualification</th>
      <th>Experience</th>
      <th>Project</th>
      <th>Hobbies</th>
      <th>Knownpl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resumes.each do |resume| %>
      <tr>
        <td><%= resume.name %></td>
        <td><%= resume.email %></td>
        <td><%= resume.dob %></td>
        <td><%= resume.address %></td>
        <td><%= resume.phone %></td>
        <td><%= resume.gender %></td>
        <td><%= resume.qualification %></td>
        <td><%= resume.experience %></td>
        <td><%= resume.project %></td>
        <td><%= resume.hobbies %></td>
        <td><%= resume.knownpl %></td>
        <td><%= link_to 'Show', resume %></td>
        <td><%= link_to 'Edit', edit_resume_path(resume) %></td>
        <td><%= link_to 'Destroy', resume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resume', new_resume_path %>
